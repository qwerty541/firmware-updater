{
    "compilerOptions": {
        "target": "ES2019",
        "module": "commonjs",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "sourceMap": true,
        "strict": true,
        "alwaysStrict": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {},
        "rootDirs": [],
        "typeRoots": [
            "./node_modules/@types"
        ],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    }
}